<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:minWidth="25px"
    android:minHeight="25px">
    <ImageView
        android:src="@drawable/Snelstart"
        android:id="@+id/SnelstartLogo8"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:layout_marginRight="95dp"
        android:paddingBottom="70dp"
        android:layout_marginBottom="-70dp" />
    <TextView
        android:id="@+id/SelecteerContacten5"
        android:text="Selecteer de contacten die u wilt toevoegen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="26sp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp" />
    <Button
        android:text="Synchroniseer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="@drawable/Borders"
        android:id="@+id/Synchroniseer3"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="10dp" />
    <SearchView
        android:id="@+id/LevZoeken4"
        android:layout_width="match_parent"
        android:background="@drawable/Borders"
        android:gravity="left"
        android:layout_marginRight="120dp"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginLeft="20dp"
        android:minWidth="25px"
        android:minHeight="25px" />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/MyListView"
        android:background="@drawable/Borders"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp" />
</LinearLayout>